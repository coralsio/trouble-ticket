{
    "name": "corals/trouble-ticket",
    "description": "Trouble Ticket",
    "keywords": [
        "Trouble",
        "Ticket"
    ],
    "homepage": "https://github.com/coralsio/trouble-ticket",
    "license": "MIT",
    "authors": [
        {
            "name": "corals",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Corals\\Modules\\TroubleTicket\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "laravel/pint": "^1.0"
    },
    "require": {
        "corals/utility-comment": "^10.0.0",
        "corals/utility-lov":"^10.0.0",
        "corals/utility-category":"^10.0.0",
        "corals/cms":"^10.0.0"
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Corals\\Modules\\TroubleTicket\\TroubleTicketServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
